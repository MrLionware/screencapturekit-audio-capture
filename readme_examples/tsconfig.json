{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "bundler",
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noEmit": true,
        "rootDir": ".."
    },
    "include": [
        "./*.ts",
        "./basics/*.ts",
        "./streams/*.ts",
        "./processing/*.ts",
        "./voice/*.ts",
        "./capture-targets/*.ts",
        "./advanced/*.ts"
    ]
}