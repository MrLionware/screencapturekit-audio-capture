{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "noEmit": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "allowJs": true,
    "checkJs": false,
    "types": ["node"],
    "rootDir": "..",
    "baseUrl": "..",
    "paths": {
      "../../dist/*": ["./dist/*"]
    }
  },
  "include": [
    "**/*.ts",
    "../dist/**/*.d.ts"
  ],
  "exclude": [
    "../node_modules"
  ]
}
